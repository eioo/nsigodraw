{
  "defaultSeverity": "error",
  "extends": ["tslint:latest", "tslint-config-prettier"],
  "rules": {
    "object-literal-sort-keys": false,
    "no-console": {
      "severity": "off"
    },
    "no-this-assignment": {
      "options": {
        "allow-destructuring": true
      }
    }
  }
}
